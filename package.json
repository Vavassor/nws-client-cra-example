{
  "name": "nws-client-cra-example",
  "version": "1.0.0",
  "description": "This repository contains a deployment of the Create React App example project in the [NWS client examples repository](https://github.com/Vavassor/nws-client-examples).",
  "main": "server.js",
  "scripts": {
    "build": "cd app && yarn build",
    "install": "cd app && yarn install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "start-app": "cd app && yarn start",
    "start:dev": "concurrently \"nodemon --ignore 'app/*'\" \"yarn start-app\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vavassor/nws-client-cra-example.git"
  },
  "author": "Andrew Dawson <dawso.andrew@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vavassor/nws-client-cra-example/issues"
  },
  "homepage": "https://github.com/Vavassor/nws-client-cra-example#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
